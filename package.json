{
  "name": "aegis-ao-rental-app",
  "version": "1.0.1",
  "description": "Car rental application with React frontend and Node.js backend",
  "main": "index.js",
  "scripts": {
    "start": "node server/index.js",
    "build": "node scripts/build-with-licenses.js",
    "build-client": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "node server/index.js",
    "client": "cd client && npm start",
    "install-all": "cd server && npm install && cd ../client && npm install",
    "sync-licenses": "node scripts/sync-blinkid-licenses.js"
  },
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "keywords": [
    "car-rental",
    "react",
    "nodejs",
    "postgresql"
  ],
  "author": "Aegis-AO Rentals",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "express-session": "^1.18.2",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.2.0"
  }
}
